{
    "name": "zamoroka/module-production-images",
    "description": "Magento 2 module: Get only necessary images from production. No need to download whole media folder",
    "minimum-stability":"stable",
    "require": {
        "php": "~7.1.0||~7.2.0||~7.3.0",
        "magento/framework": "^100.1||^101.0||^102.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5.0",
        "magento/magento-coding-standard": "^5.0"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "authors": [
        {
            "name": "Pavlo Zamoroka",
            "email": "zapashok0@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Zamoroka\\ProductionImages\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "composer",
            "url": "https://packagist.org/"
        }
    ]
}
